<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="15dp"
    tools:context=".ResultActivity">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Nama :"/>
        <TextView
            android:id="@+id/outputNama"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" outputNama"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Alamat :"/>
        <TextView
            android:id="@+id/outputAlamat"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" outputAlamat"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="No. HP :"/>
        <TextView
            android:id="@+id/outputNomor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" outputNomor"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Jenis Kelamin :"/>
        <TextView
            android:id="@+id/outputJenis"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" outputJenis"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Lokasi Pendaftaran :"/>
        <TextView
            android:id="@+id/outputLokasi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" outputLokasi"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Foto :"/>
        <ImageView
            android:id="@+id/outputFoto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" outputFoto"/>
    </LinearLayout>


</androidx.appcompat.widget.LinearLayoutCompat>